Private Sub Command1_Click()
If Rscek.State = adStateOpen Then Rscek.Close
StrSQL = " SELECT Karyawan.NIP, Karyawan.Nama, Gaji.Tingkat FROM Karyawan INNER JOIN Gaji ON Karyawan.NIP = Gaji.NIP"
Rscek.Open StrSQL, CN, adOpenStatic
Set fg.DataSource = Rscek
End Sub

Private Sub Command2_Click()
With fg
For Lrow = 1 To .Rows - 1
    StrSQL = "Update Karyawan Set Tingkat = '" & .TextMatrix(Lrow, 3) & "' where NIP = '" & .TextMatrix(Lrow, 1) & "'"
    CN.Execute StrSQL
Next
MsgBox "Finish"
.Rows = 1
End With
End Sub
